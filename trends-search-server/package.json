{
  "name": "trends-search-server",
  "version": "1.0.0",
  "private": false,
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:hjemmel/trends-search.git"
  },
  "scripts": {
    "start": "node ./bin/www",
    "start-dev": "DEBUG=trends-search-server:* nodemon ./bin/www",
    "debug": "DEBUG=trends-search-server:* nodemon --inspect ./bin/www",
    "test": "NODE_ENV=test mocha './controllers/*.js' './routes/*.js' './app.js'",
    "test-watch": "NODE_ENV=test mocha './controllers/*.js' './routes/*.js' './app.js' --watch --recursive",
    "coverage": "NODE_ENV=test nyc --per-file --reporter=text-lcov > coverage.lcov npm test"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0",
    "promise": "^8.0.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.0.2",
    "coveralls": "^3.0.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "nyc": "^12.0.2",
    "sinon": "^6.0.0"
  }
}
