    "sugarss": "^1.0.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.0.1",
    "@nivo/bar": "^0.42.1",
    "classnames": "^2.2.6",
